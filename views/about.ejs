<!DOCTYPE html>
<html lang="en" ng-app="aryalist">>
<% include header %>
    <body>
        <% include navbar %>
            <div class="">
                <div class="page-header">
                </div>
                <div  ng-controller="PersonController as person">
                    <div class="row">
                        <div class="col-md-2" ng-repeat="item in person.list.items | orderBy:'date':true">
                            <div class="shadow" style="w">
                            <img class="" src="{{item.description}}" title="{{item.title}}"></img>
                            <p class="text-center" id="Capitulo"> {{item.guid}}</p>
                        </div>
                        </div>
                    </div>
                    <div class="panel-body">

                    </div>
                </div>

                <hr>

                <% include footer %>
            </div>
            <!-- /container -->
            <script type="text/javascript">
                $(document).ready(function () {
                    $('#about').addClass("active");
                });
            </script>
            <!-- Bootstrap core JavaScript
    ================================================== -->
            <!-- Placed at the end of the document so the pages load faster -->
            <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
            <script src="https://code.jquery.com/jquery.js"></script>
            <!-- Latest compiled and minified JavaScript -->
            <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
    </body>

</html>
