<!DOCTYPE html>
<html lang="en" ng-app="showItemsapp">>
<% include header %>
    <body>
        <% include navbar %>
            <div class="">
                <div class="page-header">
                </div>
                <div  class="jumbotron" ng-controller="ItemController as item">
                    <h3 class="text-center alert alert-info">Total items: {{item.list.items.length}} </h3>
                    <div class="container">
                        <div class="col-md-2" ng-repeat="item in item.list.items | orderBy:'date':true">
                            <div class="panel panel-default shadow">
                            <img class="" src="{{item.description}}" title="{{item.title}}"></img>
                            <p class="text-center" id="Capitulo"> {{item.guid}}</p>
                        </div>
                        </div>
                    </div>
                    <div class="panel-body">

                    </div>
                </div>

                <hr>

                <% include footer %>
            </div>
            <!-- /container -->
            <script type="text/javascript">
                $(document).ready(function () {
                    $('#about').addClass("active");
                });
            </script>
            <!-- Bootstrap core JavaScript
    ================================================== -->
            <!-- Placed at the end of the document so the pages load faster -->
            <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
            <script src="https://code.jquery.com/jquery.js"></script>
            <!-- Latest compiled and minified JavaScript -->
            <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
    </body>

</html>
